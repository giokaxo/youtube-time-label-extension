{
  "manifest_version": 3,
  "name": "Youtube remove time label",
  "description": "Remove time label for youtube video previews.",
  "version": "1.0",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-64.png",
    "128": "images/icon-128.png"
  },
  "action": {
    "default_icon": {
      "16": "images/icon-16.png",
      "32": "images/icon-32.png",
      "48": "images/icon-64.png",
      "128": "images/icon-128.png"
    }
  },
  "content_scripts": [
    {
      "js": [
        "script.js"
      ],
      "matches": [
        "https://www.youtube.com/*"
      ]
    }
  ],
  "permissions": [
    "scripting",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  }
}